  <!-- About Section -->
    <section class="success" id="about">
        <div class="container">
            <div class="row">
                <div id="store_output">
                    
                        <% @preference.each do |store_name, store_details| %>
                           <h2> <%= store_name %> </h2>
                            <br>
                            <h5 class="store_description"><% store_details.each do |key, value| %>
                                <%= key %>: 
                                <div class="output_text">
                                <% if key == :website %>
                                    <a class="output_link" target="_blank" href = <%= value %>> <%= value %> </a>
                                <% elsif key == :name %>  
                                   <%= value.gsub("_", " ") %>
                                
                                <% elsif key == :address %>
                                
                                    
                                    <a class="output_link" target="_blank" href = <%= "https://www.google.com/maps/place/#{value.gsub(" ", "+")}"%>> <%= value%> </a>
                                      <br><br>
                                     <iframe
                                      width="500"
                                      height="300"
                                      frameborder="0" style="border:0"
                                      src= <%= "https://www.google.com/maps/embed/v1/place?key=#{ENV['GOOGLE_API']}&q=#{value.gsub(" ", "+")}"%> allowfullscreen>
                                    </iframe>
                                    <br>
                                  
                                <% else %>
                                    <%= value %>
                                    
                                <% end %>
                                
                                </div>
                                <br>
                            <% end %></h5>
                            <br>
                        <% end %>
                     <br>
                </div>
            </div>
            <br>
            <div class="row">
                <div id="blurb">
                    <p>Enjoy shopping with Comet!</p>
                  </div>
            </div>
        </div>
    </section>




